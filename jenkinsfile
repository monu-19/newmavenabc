pipeline
{
    agent any
    stages
    {
        stage('ContinuousDownload Loans')
        {
            steps
            {
                 git 'https://github.com/monu-19/newmavenabc.git'
            }
        }
        stage('ContinuousBuild Loans')
        {
            steps
            {
               sh 'mvn package'
            }
        }
    }
}
